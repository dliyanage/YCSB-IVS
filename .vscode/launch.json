{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Debug YCSB Workload",
      "request": "launch",
      "mainClass": "site.ycsb.Client",
      "args": ["-t", "-db", "site.ycsb.db.rocksdb.RocksDBClient", "-s", "-P", "workloads/workloada-extend", "-p", "rocksdb.dir=/tmp/rocksdb_data"],
      "projectName": "core",
      "classPaths": [
        "${workspaceFolder}/conf",
        "${workspaceFolder}/core/target/core-0.18.0-SNAPSHOT.jar",
        "${workspaceFolder}/core/target/dependency/HdrHistogram-2.1.4.jar",
        "${workspaceFolder}/core/target/dependency/htrace-core4-4.1.0-incubating.jar",
        "${workspaceFolder}/core/target/dependency/jackson-core-asl-1.9.4.jar",
        "${workspaceFolder}/core/target/dependency/jackson-mapper-asl-1.9.4.jar",
        "${workspaceFolder}/rocksdb/target/rocksdb-binding-0.18.0-SNAPSHOT.jar",
        "${workspaceFolder}/rocksdb/target/dependency/jcip-annotations-1.0.jar",
        "${workspaceFolder}/rocksdb/target/dependency/slf4j-api-1.7.25.jar",
        "${workspaceFolder}/rocksdb/target/dependency/rocksdbjni-6.2.2.jar",
        "${workspaceFolder}/rocksdb/target/dependency/slf4j-simple-1.7.25.jar"
      ],
      "sourcePaths": [
        "${workspaceFolder}/core/src/main/java",
        "${workspaceFolder}/rocksdb/src/main/java"
      ],
      "env": {
        "YCSB_HOME": "${workspaceFolder}"
      }
    }
  ]
}


