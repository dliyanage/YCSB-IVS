{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "java",
        "name": "Debug YCSB Workload",
        "request": "launch",
        "mainClass": "site.ycsb.Client",
        "args": ["-t", "-db", "site.ycsb.db.MongoDbClient", "-s", "-P", "workloads/workloada-extend"],
        "projectName": "core",
        "classPaths": [
          "${workspaceFolder}/conf",
          "${workspaceFolder}/core/target/core-0.18.0-SNAPSHOT.jar",
          "${workspaceFolder}/core/target/dependency/HdrHistogram-2.1.4.jar",
          "${workspaceFolder}/core/target/dependency/htrace-core4-4.1.0-incubating.jar",
          "${workspaceFolder}/core/target/dependency/jackson-core-asl-1.9.4.jar",
          "${workspaceFolder}/core/target/dependency/jackson-mapper-asl-1.9.4.jar",
          "${workspaceFolder}/mongodb/target/mongodb-binding-0.18.0-SNAPSHOT.jar",
          "${workspaceFolder}/mongodb/target/dependency/logback-classic-1.1.2.jar",
          "${workspaceFolder}/mongodb/target/dependency/logback-core-1.1.2.jar",
          "${workspaceFolder}/mongodb/target/dependency/mongo-java-driver-3.11.0.jar",
          "${workspaceFolder}/mongodb/target/dependency/mongodb-async-driver-2.0.1.jar",
          "${workspaceFolder}/mongodb/target/dependency/slf4j-api-1.7.25.jar",
          "${workspaceFolder}/mongodb/target/dependency/snappy-java-1.1.7.1.jar",
        ],
        "sourcePaths": [
          "${workspaceFolder}/core/src/main/java",
          "${workspaceFolder}/mongodb/src/main/java"
        ],
        "env": {
          "YCSB_HOME": "${workspaceFolder}"
        }
      }
    ]
  }
    